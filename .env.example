# Hyperliquid Lambda .env example
# Copy this file to .env and replace with your values

# Your Hyperliquid wallet private key
HYPERLIQUID_PRIVATE_KEY=your_private_key_here

# Password for TradingView webhook authentication
WEBHOOK_PASSWORD=your_secure_webhook_password_here

# Use mainnet or testnet (true/false)
HYPERLIQUID_USE_MAINNET=false
